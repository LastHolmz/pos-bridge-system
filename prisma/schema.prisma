datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  barcode      String   @unique
  name         String
  buyingPrice  Int
  sellingPrice Int
  qty          Int
  unit         Int
  expiryDate   DateTime
}
